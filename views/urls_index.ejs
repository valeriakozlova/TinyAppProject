<!-- views/urls_index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>

  <header>
    <% include ./partials/_header %>
  </header>

<ul>
  <% for (var url in urls) { %>
  <li><%= url %> <%= urls[url] %> <a href="#">Shorten your URL</a></li>
  <% }; %>
</ul>

</body>
</html>


